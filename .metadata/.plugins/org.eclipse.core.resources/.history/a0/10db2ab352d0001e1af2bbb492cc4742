package org.kh.site2;
// 대입 연산자 : 연산 후에 대입하는 연산자 : +=, -=, *=, /=, %=, &=, |=, >>=, <<=, >>>=, ^= 
public class Operation7 {

	public static void main(String[] args) {

		int a = 20;
		int b = 15;
		int tmp;
		a += b; // a = a + b (a = 35, b = 15)
		b -= a; // b = b - a (a = 35, b = -20)
		System.out.println("a 와 b의 값은 : " + a + "과" + b);
		// a 와 b의 값을 맞교환
		tmp = a; // c의 역할 : temp값
		a = b;
		b = tmp;
		int x = 0b01101;
		int y = 0b01011;
		

		
	}

}
