package org.kh.java;

public class Variable2 {

	public static void main(String[] args) {

		byte b = 10;
		short s = 20;
		int i; // 값이 없으면 null 오류
		long l;
		System.out.println("b = " + b);
		System.out.println("s = " + s);
		System.out.println("i = " + i);
		System.out.println("l = " + l);



	}

}
