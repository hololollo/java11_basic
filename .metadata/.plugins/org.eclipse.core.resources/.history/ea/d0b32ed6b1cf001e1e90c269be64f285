package org.kh.java;

public class Variable7 {

	public static void main(String[] args) {

		String name = "강범준";
		Int age = 32;
		float height = 177.1f;
		boolean pass = true;
		double weight = 69.3;
		
		System.out.println("name = " + name.getClass().getName());
		System.out.println("age = " + age.getClass().getAge());
		

	}

}
