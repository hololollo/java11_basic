package org.kh.app;
// 조건문 if~else~ : if(조건식) { 조건식이 참일때 실행문; } else { 조건이 거짓일 때 실행문; }
public class Condition2 {

	public static void main(String[] args) {
		char sel = '1';
		// 성별코드(sel)가 1이거나 3이면, "남성", 아니면 "여성"
		
		String male;
		
		if (sel == '1' || sel == '3') {
			System.out.println("남성");
		} else {
			System.out.println("여성");
		}
	}

}
