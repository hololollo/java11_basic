package org.kh.app;
// 조건문 if
public class Condition1 {

	public static void main(String[] args) {
		int point = 80;
		String pass = "불합격";
		// 판정(pass)은 점수가 80\\70점 이상이면 "합격" 아니면 "불합격"
		
		if (point >= 70) {
			System.out.println("pass : 합격");
		} else {
			System.out.println("pass : 불합격");
		}
		
		
		
	}

}
